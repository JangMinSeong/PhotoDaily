<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.photodaily">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.PhotoDaily">
        <activity android:name=".ui.main.DayPhotoAdd"></activity>
        <activity android:name=".ui.main.TravelSelectRegion" />
        <activity android:name=".ui.main.DayDiaryOne" />
        <activity
            android:name=".TabNavigator"
            android:label="@string/title_activity_tab_navigator"
            android:theme="@style/Theme.PhotoDaily.NoActionBar" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>